{
  "compilerOptions": {
    "outFile": "dist/bundle.js",
    "rootDir": "src",
    "noEmitOnError": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "sourceMap": true,
    "target": "es5",
    "moduleDetection": "force",
    "lib": ["DOM", "ES2016"],
    "esModuleInterop": true,
    "removeComments": false,
  },
  "include": ["src"],
  "exclude": []
}
